<h1>Listing typeconnects</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @typeconnects.each do |typeconnect| %>
  <tr>
    <td><%= typeconnect.name %></td>
    <td><%= link_to 'Show', typeconnect %></td>
    <td><%= link_to 'Edit', edit_typeconnect_path(typeconnect) %></td>
    <td><%= link_to 'Destroy', typeconnect, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Typeconnect', new_typeconnect_path %>
